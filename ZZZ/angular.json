{
    "version": 1,
    "projects": {
        "store": {
            "projectType": "library",
            "root": "libs/store",
            "sourceRoot": "libs/store/src",
            "prefix": "ngx-ui-tools",
            "architect": {
                "build": {
                    "builder": "@nrwl/angular:package",
                    "options": {
                        "tsConfig": "libs/store/tsconfig.lib.json",
                        "project": "libs/store/ng-package.json"
                    },
                    "configurations": {
                        "production": {
                            "tsConfig": "libs/store/tsconfig.lib.prod.json"
                        }
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": ["libs/store/tsconfig.lib.json", "libs/store/tsconfig.spec.json"],
                        "exclude": ["**/node_modules/**", "!libs/store/**/*"]
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "libs/store/src/test.ts",
                        "tsConfig": "libs/store/tsconfig.spec.json",
                        "karmaConfig": "libs/store/karma.conf.js"
                    }
                }
            },
            "schematics": {
                "@schematics/angular:component": {
                    "style": "scss"
                }
            }
        },
        "module-federation": {
            "projectType": "library",
            "root": "libs/module-federation",
            "sourceRoot": "libs/module-federation/src",
            "prefix": "ngx-ui-tools",
            "architect": {
                "build": {
                    "builder": "@nrwl/angular:package",
                    "options": {
                        "tsConfig": "libs/module-federation/tsconfig.lib.json",
                        "project": "libs/module-federation/ng-package.json"
                    },
                    "configurations": {
                        "production": {
                            "tsConfig": "libs/module-federation/tsconfig.lib.prod.json"
                        }
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "libs/module-federation/tsconfig.lib.json",
                            "libs/module-federation/tsconfig.spec.json"
                        ],
                        "exclude": ["**/node_modules/**", "!libs/module-federation/**/*"]
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "libs/module-federation/src/test.ts",
                        "tsConfig": "libs/module-federation/tsconfig.spec.json",
                        "karmaConfig": "libs/module-federation/karma.conf.js"
                    }
                }
            },
            "schematics": {
                "@schematics/angular:component": {
                    "style": "scss"
                }
            }
        }
    },
    "cli": {
        "defaultCollection": "@nrwl/angular"
    },
    "schematics": {
        "@nrwl/workspace": {
            "library": {
                "linter": "tslint"
            }
        },
        "@nrwl/cypress": {
            "cypress-project": {
                "linter": "tslint"
            }
        },
        "@nrwl/node": {
            "application": {
                "linter": "tslint"
            },
            "library": {
                "linter": "tslint"
            }
        },
        "@nrwl/nest": {
            "application": {
                "linter": "tslint"
            },
            "library": {
                "linter": "tslint"
            }
        },
        "@nrwl/express": {
            "application": {
                "linter": "tslint"
            },
            "library": {
                "linter": "tslint"
            }
        },
        "@nrwl/angular:application": {
            "unitTestRunner": "karma",
            "e2eTestRunner": "cypress"
        },
        "@nrwl/angular:library": {
            "unitTestRunner": "karma"
        }
    },
    "defaultProject": "store"
}
