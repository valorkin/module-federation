{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "module": "esnext",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2016",
      "dom"
    ],
    "esModuleInterop": true,
    "paths": {
      "@valorkin/mf-core": [
        "libs/mf-core/src/index.ts"
      ],
      "@valorkin/mf-rpc": [
        "libs/mf-rpc/src/index.ts"
      ],
      "@valorkin/ngx-mf-injector": [
        "libs/ngx-mf-injector/src/index.ts"
      ]
    }
  }
}
