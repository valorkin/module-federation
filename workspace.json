{
  "version": 2,
  "projects": {
    "cflp-app": {
      "root": "packages/cflp-app",
      "type": "library"
    },
    "content-item-app": {
      "root": "packages/content-item-app",
      "type": "library"
    },
    "content-recommended-categories": {
      "root": "packages/content-recommended-categories",
      "type": "library"
    },
    "content-req-app": {
      "root": "packages/content-req-app",
      "type": "library"
    },
    "iframe-source-app": {
      "root": "packages/iframe-source-website",
      "type": "library"
    },
    "ngrx-app": {
      "root": "packages/ngrx-app",
      "type": "library"
    },
    "nx-app": {
      "root": "packages/nx-app",
      "type": "library"
    },
    "one-bx-shell-app": {
      "root": "packages/one-bx-shell-app",
      "type": "library"
    },
    "webcomp-auth-app": {
      "root": "packages/webcomp-auth-app",
      "type": "library"
    },
    "mf-core": {
      "projectType": "library",
      "root": "libs/mf-core",
      "sourceRoot": "libs/mf-core/src",
      "prefix": "mf-root",
      "targets": {
        "build": {
          "executor": "@nrwl/angular:package",
          "options": {
            "tsConfig": "libs/mf-core/tsconfig.lib.json",
            "project": "libs/mf-core/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/mf-core/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/mf-core/src/**/*.ts",
              "libs/mf-core/src/**/*.html"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/mf-core"
          ],
          "options": {
            "jestConfig": "libs/mf-core/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "generators": {
    "@nrwl/angular:application": {
      "style": "css",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:library": {
      "style": "css",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "css"
    }
  }
}